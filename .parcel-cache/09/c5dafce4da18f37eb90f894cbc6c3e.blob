#page-header {
  position: absolute;
  display: grid;
  grid-template-columns: 100%;
  grid-template-rows: repeat(2, 1fr);
  margin: auto;
  margin-top: 5vh;
  width: 100vw;
  height: 40vh;
}

#title-text-container {
  margin: auto;
  width: auto;
  height: 50%;
}

#title-text {
  margin: auto;
  text-align: center;
  font-family: Staatliches, cursive;
  color: #9db1c0;
  font-size: 9rem;
  letter-spacing: 0.5rem;
  text-transform: uppercase;
}

#subtitle-text-container {
  margin: auto;
  width: auto;
  height: 50%;
}

#subtitle-text {
  margin: auto;
  text-align: center;
  font-family: Ubuntu, sans-serif;
  color: #9db1c0;
  font-size: 3rem;
  text-transform: uppercase;
}

html,
body {
  margin: 0;
  padding: 0;
}

body {
  background-color: #293e5a;
  scroll-behavior: smooth;
  overflow: hidden;
}

body::-webkit-scrollbar {
  display: none;
}

#particles-js {
  width: 100%;
  height: 100%;
}
