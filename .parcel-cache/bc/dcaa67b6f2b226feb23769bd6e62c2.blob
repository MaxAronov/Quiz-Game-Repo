#page-header {
  position: absolute;
  display: flex;
  justify-content: center;
  margin: auto;
  margin-top: 5vh;
  width: 90vw;
  height: 40vh;
  background-color: #343d4b;
  opacity: 0.5;
}

#title-text-container {
  display: flex;
  justify-content: center;
