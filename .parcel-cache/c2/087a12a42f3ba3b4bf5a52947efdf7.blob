  margin-top: 5vh;
  width: auto;
  height: auto;
}

#title-text-container {
  width: auto;
  height: auto;
}

#title-text {
  text-align: center;
  font-family: Staatliches, cursive;
  color: #9db1c0;
  font-size: 7.5rem;
